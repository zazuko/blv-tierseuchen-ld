{
  "name": "blv-tierseuchen-ld",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "standard && jest",
    "start": "mkdir -p target source; barnard59 run --pipeline urn:pipeline:tierseuchen-ld#Root --format text/turtle pipelines/blv-tierseuchen.ttl",
    "startFile": "mkdir -p target source; barnard59 run --pipeline urn:pipeline:tierseuchen-ld#RootFile --format text/turtle pipelines/blv-tierseuchen.ttl",
    "clean-nt": "rm -f target/everything.nt && cat target/*.nt | sed '/_:/d' | sed '/<file/d' | sed '/#(No/d' | sed '/NULL/d'  > target/everything.nt",
    "pipeline": "npm start -- --pipeline",
    "animalpest-pipeline-file": "barnard59 run -v --format text/turtle --pipeline=urn:pipeline:tierseuchen-ld#RootFile pipelines/blv-tierseuchen.ttl",
    "animalpest-pipeline-test": "barnard59 run -v --variable=\"endpoint=https://stardog-test.cluster.ldbar.ch/lindas\" --variable=\"user=$STARDOG_USERNAME\" --variable=\"password=$STARDOG_PASSWORD_TEST\" --format text/turtle --pipeline=urn:pipeline:tierseuchen-ld#Root pipelines/blv-tierseuchen.ttl",
    "animalpest-pipeline-int": "barnard59 run -v --variable=\"endpoint=https://stardog-int.cluster.ldbar.ch/lindas\" --variable=\"user=$STARDOG_USERNAME\" --variable=\"password=$STARDOG_PASSWORD_INT\" --format text/turtle --pipeline=urn:pipeline:tierseuchen-ld#Root pipelines/blv-tierseuchen.ttl",
    "animalpest-pipeline-prod": "barnard59 run -v --variable=\"endpoint=https://stardog.cluster.ldbar.ch/lindas\" --variable=\"user=$STARDOG_USERNAME\" --variable=\"password=$STARDOG_PASSWORD_PROD\" --format text/turtle --pipeline=urn:pipeline:tierseuchen-ld#Root pipelines/blv-tierseuchen.ttl"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zazuko/blv-tierseuchen-ld.git"
  },
  "author": "Tomasz Pluskiewicz <tomasz@t-code.pl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zazuko/blv-tierseuchen-ld/issues"
  },
  "homepage": "https://github.com/zazuko/blv-tierseuchen-ld#readme",
  "dependencies": {
    "barnard59": "0.0.5",
    "barnard59-formats": "0.0.6",
    "barnard59-graph-store": "0.0.4",
    "barnard59-tdb": "git+https://github.com/zazuko/barnard59-tdb.git",
    "node-fetch": "^2.6.0",
    "rdf-ext": "^1.3.0",
    "unzipper": "^0.9.11",
    "url-slug": "^2.2.0"
  },
  "devDependencies": {
    "standard": "^12.0.1"
  }
}
